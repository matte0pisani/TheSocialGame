                                     RelativeLayout.XConstraint="{ConstraintExpression Type=RelativeToParent, Property=Width, Factor=0}"
                                     RelativeLayout.YConstraint="{ConstraintExpression Type=RelativeToParent, Property=Height, Factor=0.7}">
                                <Button.FontSize>
                                      <OnPlatform x:TypeArguments="x:Double" Android="25" iOS="23"/>
                                </Button.FontSize>
                             </Button>
                        
                         </RelativeLayout>
                     </Frame>

                    <Frame x:Name="ConfermaEliminazioneFrame" CornerRadius="20" BackgroundColor="BlanchedAlmond" Padding="8" HasShadow="True" BorderColor="{StaticResource FirstColor}"
